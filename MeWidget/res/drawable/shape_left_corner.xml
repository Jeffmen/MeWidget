<?xml version="1.0" encoding="UTF-8"?>  
<!-- 底部圆角 白色背景 灰色边框 长方体 -->  
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">   
    <item android:top="1dp" android:bottom="1dp" android:left="1dp">  
        <shape>  
            <solid android:color="#CD3333" />  
            <corners android:topLeftRadius="10dp" android:topRightRadius="0.1dp"  
                android:bottomRightRadius="0.1dp" android:bottomLeftRadius="10dp" />  
            <stroke android:width="1dp" android:color="#CD3333" />  
        </shape>  
    </item>  
</layer-list>
